<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_focused="false" android:state_selected="false" android:state_pressed="false" android:drawable="@drawable/icon_history_default"/>
    <item android:state_focused="false" android:state_selected="true" android:state_pressed="false" android:drawable="@drawable/icon_history_higlight"/>
    <!--FOCUSED STATES-->
    <item android:state_focused="true" android:state_selected="false" android:state_pressed="false" android:drawable="@drawable/icon_history_higlight"/>
    <item android:state_focused="true" android:state_selected="true" android:state_pressed="false" android:drawable="@drawable/icon_history_higlight"/>
    <!--PRESS-->
    <item android:state_selected="true" android:state_pressed="true" android:drawable="@drawable/icon_history_higlight"/>
    <item android:state_pressed="true" android:drawable="@drawable/icon_history_default"/>

</selector>